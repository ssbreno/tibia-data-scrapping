{
  "name": "nestjs-template",
  "private": true,
  "version": "1.0.0",
  "description": "Back-end template to use to build a NestJS application",
  "author": "Breno Sobral",
  "license": "ISC",
  "engines": {
    "node": "^18.17.0 || >=20.5.0",
    "npm": "^10.3.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --forceExit --detectOpenHandles --config ./test/jest-e2e.json",
    "doc": "npx @compodoc/compodoc -p tsconfig.json",
    "doc:serve": "pnpm run doc && npx @compodoc/compodoc -s",
    "precommit": "lint-staged && pnpm run lint",
    "prepush": "pnpm run test",
    "prepare": "is-ci || husky install",
    "commit": "cz"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@fastify/compress": "^6.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/request-context": "^5.1.0",
    "@fastify/static": "^6.12.0",
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-fastify": "^10.3.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/terminus": "^10.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cli-color": "^2.0.3",
    "fastify": "^4.25.2",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "uuidv4": "^6.2.13",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@compodoc/compodoc": "^1.1.23",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/cli-color": "^2.0.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "semantic-release": "^22.0.12",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
